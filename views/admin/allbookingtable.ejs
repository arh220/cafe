<!DOCTYPE html>
<html lang="en">

<head>
    <%- include("./partials/head.ejs") %>
        <title>Admin</title>
</head>

<body>
    <%- include("./partials/sidebar.ejs") %>

        <!-- ! Main -->
        <main class="main users chart-page" id="skip-target">
            <div class="container">
                <h2 class="main-title">Dashboard</h2>
                <div class="row">
                    <div class="col-lg-9">
                        <div class="users-table table-wrapper">
                            <table class="posts-table">
                                <thead>
                                    <tr class="users-table-info">
                                        <th>No</th>
                                        <th>Name</th>
                                        <th>Member</th>
                                        <th>Date</th>
                                        <th>Time</th>
                                        <th>Mobile</th>
                                        <th>Msg</th>
                                        <th>Action</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% if (allTable && allTable.length>0) { %>
                                        <% allTable.forEach((item,index)=> { %>
                                            <tr>
                                                <td>
                                                    <%= index+1 %>
                                                </td>

                                                <td>
                                                    <%= item.name %>
                                                </td>
                                                <td>
                                                    <%= item.member %>
                                                </td>
                                                <td>
                                                    <%= item.date %>
                                                </td>
                                                <td>
                                                    <%= item.time %>
                                                </td>
                                                <td>
                                                    <%= item.mo %>
                                                </td>
                                                <td>
                                                    <%= item.msg %>
                                                </td>
                                                <td>
                                                    <a href="/admin/deltbl/<%= item._id %>"><i
                                                            class="fa-solid fa-trash "></i></a>
                                                </td>
                                            </tr>
                                            <% }) %>
                                                <% } %>
                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
        </main>
        <%- include("./partials/footer.ejs") %>
            <%- include("./partials/script.ejs") %>

</body>

</html>